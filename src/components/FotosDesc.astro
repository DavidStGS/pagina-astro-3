---
import CardsFirst from "./CardsFirst.astro";
import { getDataFirst } from "../lib/FetchData";

export const lanzamientoss = await getDataFirst(70);
---

<div class="grid gap-8 sm:grid-cols-2 lg:grid-cols-3">
  {
    lanzamientoss.map(
      ({ id, flight_number: FlightNumber, links, details, success }) => (
        <article>
          <CardsFirst
            id={id}
            flightNumber={FlightNumber}
            img={links.patch.small}
            details={details}
            success={success}
          />
        </article>
      )
    )
  }
</div>
